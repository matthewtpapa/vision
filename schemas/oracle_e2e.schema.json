{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://latency.vision/schemas/oracle_e2e.schema.json",
  "title": "Latency Vision End-to-End Oracle Bench Metrics",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "bench",
    "k",
    "evaluated_queries",
    "known_queries",
    "p_at_1",
    "e2e_p95_ms",
    "e2e_p99_ms"
  ],
  "properties": {
    "schema_version": {"type": "string"},
    "bench": {"type": "string"},
    "k": {"type": "integer", "minimum": 1},
    "evaluated_queries": {"type": "integer", "minimum": 0},
    "known_queries": {"type": "integer", "minimum": 0},
    "p_at_1": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0
    },
    "e2e_p95_ms": {
      "type": "number",
      "minimum": 0.0
    },
    "e2e_p99_ms": {
      "type": "number",
      "minimum": 0.0
    }
  }
}
